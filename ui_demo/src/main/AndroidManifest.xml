<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.xiaocoder.android_ui_demo">

    <!-- popwindow -->
    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" />
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ExpandListActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".pop.PopActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".CircleProgressBarActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".DialogActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".DialogSytleActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".LineProgressBarActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ProgressViewActivity"
            android:screenOrientation="portrait" />
        <activity android:name=".drawer.DrawerActivity" />
        <activity android:name=".pickerview.PickerViewActivity" />
        <activity android:name=".SwitchButtonActivity" />
        <activity android:name=".PagerSlidingTabActivity" />
        <activity android:name=".RoundImageViewActivity" />
        <activity android:name=".ListView2GridViewActivity" />
        <activity android:name=".HListViewActivity" />
        <activity android:name=".ClipImageActivity" />
        <activity android:name=".DoubleCircleButtonActivity" />
        <activity android:name=".ScrollActivity" />
        <activity android:name=".ImagesZoomActivity" />
        <activity android:name=".SwipeAdapterActivity" />
        <activity android:name=".TabActivity" />
        <activity android:name=".WebViewActivity"/>
    </application>

</manifest>